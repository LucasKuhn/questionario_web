<% aplicacao = AplicacoesController.aplicacao %>
<h2>Aplicacao #<%= aplicacao.id %></h2>
<h4 class="font-weight-light"><%= aplicacao.percentual_acerto %> de acerto </h4>
<hr class="mt-0">

<div class="row my-4">
  <div class="col-md-6">
    <p>
      <strong>Data:</strong><br>
      <%= aplicacao.data %>
    </p>
    <p>
      <strong>Inicio:</strong><br>
      <%= aplicacao.hora_inicio %>
    </p>
    <p>
      <strong>Fim:</strong><br>
      <%= aplicacao.hora_inicio %>
    </p>
  </div>
</div>
